module tb_xnor2_4b(

    );
    
    reg [3:0] a, b;
    wire [3:0] y;
    
    xnor2_4b_gen U0 (a, b, y);
    
    initial begin
    a=4'b0001; b=4'b1000;
    #10 a=4'b1010; b=4'b0101;
    #10 a=4'b1001; b=4'b0111;
    #10 a=4'b0011; b=4'b1110;
    #10 a=4'b1100; b=4'b0110;
    #10 a=4'b1110; b=4'b1111;
    #10 a=4'b1111; b=4'b0000;
    #20 $finish;
    end
    
endmodule
